    "start": "electron .",
    "build-portable": "electron-builder --win --portable",
    "build-dir": "electron-builder --win --dir"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "build": {
    "appId": "com.miprimerdia.app",
    "productName": "Mi Primer DÃ­a",
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "assets/Miprimerdia.ico",
      "target": [
        {
          "target": "portable",
          "arch": ["x64"]
